<!doctype html>
<!--[if lt IE 7]> <html class="no-js ie6" lang="en"> <![endif]-->
<!--[if IE 7]>    <html class="no-js ie7" lang="en"> <![endif]-->
<!--[if IE 8]>    <html class="no-js ie8" lang="en"> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js" lang="en"> <!--<![endif]-->
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <title><%= title %></title>
  <meta name="description" content="<%= meta.desc %>">
  <meta name="author" content="<%= meta.author.name + ' ' + meta.author.url %>">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">

  <!--[if lt IE 9]>
    <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
  <![endif]-->
  <script>document.documentElement.className = 'js';</script>

  <link rel="stylesheet" href="/stylesheets/style.css">
</head>
<body>
  <div id="container">
    <header>
      <%= title %> - <%= meta.desc %>
    </header>
    <noscript>JavaScript is required!</noscript>
    <div id="main" role="main">
      <%- body %>
    </div>
    <footer>
      <strong><%= title %></strong>
      v<em><%= meta.version %></em>
      by <a href="<%= meta.author.url %>" target="_blank"><%= meta.author.name %></a>.
    </footer>
  </div> <!--! end of #container -->

<% if(jquery) { %>
  <script src="//ajax.googleapis.com/ajax/libs/jquery/<%= jquery.version %>/jquery.js"></script>
  <script>window.jQuery || document.write("<script src='/javascripts/libs/jquery-<%= jquery.version %>.min.js'>\x3C/script>")</script>
  <!--[if lt IE 8 ]>
    <script src="https://github.com/douglascrockford/JSON-js/raw/master/json2.js"></script>
  <![endif]-->
<% } %>
  <script>
    // make it safe to use console.log always
    (function(b){function c(){}for(var d="assert,count,debug,dir,dirxml,error,exception,group,groupCollapsed,groupEnd,info,log,markTimeline,profile,profileEnd,time,timeEnd,trace,warn".split(","),a;a=d.pop();)b[a]=b[a]||c})(window.console=window.console||{});
  </script>

<% if(scripts) for (var c = 0; c < scripts.length; c++) { %>
  <script src="<%= (scripts[c].match(/^(https?:)?\//i) ? '' : '/javascripts/') + scripts[c] %>"></script>
<% } %>


<!--[if lt IE 7 ]>
  <script src="/javascripts/libs/dd_belatedpng.js"></script>
  <script>DD_belatedPNG.fix("img, .png_bg");</script>
<![endif]-->

<% if(status!=404 && meta.github) { %>
  <a href="<%= meta.github %>" target="_blank"><img style="position: absolute; top: 0; right: 0; border: 0;" src="https://d3nwyuy0nl342s.cloudfront.net/img/30f550e0d38ceb6ef5b81500c64d970b7fb0f028/687474703a2f2f73332e616d617a6f6e6177732e636f6d2f6769746875622f726962626f6e732f666f726b6d655f72696768745f6f72616e67655f6666373630302e706e67" alt="Fork me on GitHub"></a>
<% } %>

</body>
</html>
